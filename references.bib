@Article{li_law_2014,
  author        = {Li, Songzi and Li, Xiang-Dong and Xie, Yong-Xiao},
  title         = {On the {{Law}} of {{Large Numbers}} for the Empirical Measure Process of {{Generalized Dyson Brownian}} Motion},
  eprint        = {1407.7234},
  eprinttype    = {arxiv},
  note          = {00004},
  abstract      = {We study the generalized Dyson Brownian motion (GDBM) of an interacting \$N\$-particle system with logarithmic Coulomb interaction and general potential \$V\$. Under reasonable condition on \$V\$, we prove the existence and uniqueness of strong solution to SDE for GDBM. We then prove that the family of the empirical measures of GDBM is tight on \$$\backslash$mathcal \{C\}([0,T],$\backslash$mathscr\{P\}($\backslash$mathbb\{R\}))\$ and all the large \$N\$ limits satisfy a nonlinear McKean-Vlasov equation. Inspired by previous works due to Biane and Speicher, Carrillo, McCann and Villani, we prove that the McKean-Vlasov equation is indeed the gradient flow of the Voiculescu free entropy on the Wasserstein space of probability measures over \$$\backslash$mathbb\{R\}\$. Using the optimal transportation theory, we prove that if \$V"$\backslash$geq K\$ for some constant \$K$\backslash$in $\backslash$mathbb\{R\}\$, the McKean-Vlasov equation has a unique weak solution. This proves the Law of Large Numbers and the propagation of chaos for the empirical measures of GDBM. Finally, we prove the longtime convergence of the McKean-Vlasov equation for \$C\^2\$-convex potentials \$V\$.},
  annote        = {Comment: Update revised version of the previous version},
  archiveprefix = {arXiv},
  journal       = {arXiv:1407.7234 [math]},
  keywords      = {Determinantal, Transport},
  month         = jul,
  primaryclass  = {math},
  year          = {2014},
}

