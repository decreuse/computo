@Article{anderes_discrete_2015,
  author        = {Anderes, Ethan and Borgwardt, Steffen and Miller, Jacob},
  title         = {Discrete {{Wasserstein Barycenters}}: {{Optimal Transport}} for {{Discrete Data}}},
  eprint        = {1507.07218},
  eprinttype    = {arxiv},
  abstract      = {Wasserstein barycenters correspond to optimal solutions of transport problems for several marginals, and as such are at the core of applications ranging from economics to statistics and computer science. The corresponding theory for data in the form of continuous probability measures is well-developed. However, exact computations in this setting are tractable in only a small number of specialized cases. On the other hand, in many applications of economics, data is given as a set of probability measures with finite support; in other situations, continuous data is discretized to allow for the use of state-of-the-art heuristics. Such a discrete input format allows us to study the properties and computation of barycenters from the point of view of polyhedral theory. In doing so, we are able to mirror some of the best properties of barycenters for the continuous setting and obtain several remarkable differences: barycenters for discrete data have a provably sparse finite support and do not split mass in their optimal transport maps. We illustrate the results of the discrete barycenter theory with some proof-of-concept computations of exact barycenters, highlighting the observed properties in an economic toy example.},
  archiveprefix = {arXiv},
  file          = {/Users/laurent/Documents/zotero/storage/TEKMJRMB/1507.html},
