@Article{Privault2022,
  author       = {Privault, N. and Serafin, G.},
  date         = {2022-01},
  journaltitle = {Electronic Journal of Probability},
  title        = {Berry-{Esseen} bounds for functionals of independent random variables},
  doi          = {10.1214/22-EJP795},
