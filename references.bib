@Article{Poplavskyidistributionlargestreal2016,
  author        = {Poplavskyi, M. and Tribe, Roger and Zaboronski, Oleg},
  title         = {On the Distribution of the Largest Real Eigenvalue for the Real {{Ginibre}} Ensemble},
  eprint        = {1603.05849},
  eprinttype    = {arxiv},
  abstract      = {Let \$$\backslash$sqrt\{N\}+$\backslash$lambda\_\{max\}\$ be the largest real eigenvalue of a random \$N$\backslash$times N\$ matrix with independent \$N(0,1)\$ entries (the `real Ginibre matrix'). We study the large deviations behaviour of the limiting \$N$\backslash$rightarrow $\backslash$infty\$ distribution \$P[$\backslash$lambda\_\{max\}$<$t]\$ of the shifted maximal real eigenvalue \$$\backslash$lambda\_\{max\}\$. In particular, we prove that the right tail of this distribution is Gaussian: for \$t$>$0\$, $\backslash$[ P[$\backslash$lambda\_\{max\}$<$t]=1-$\backslash$frac\{1\}\{4\}$\backslash$mbox\{erfc\}(t)+O$\backslash$left(e\^\{-2t\^2\}$\backslash$right). $\backslash$] This is a rigorous confirmation of the corresponding result of Forrester and Nagao. We also prove that the left tail is exponential: for \$t$<$0\$, $\backslash$[ P[$\backslash$lambda\_\{max\}$<$t]= e\^\{$\backslash$frac\{1\}\{2$\backslash$sqrt\{2$\backslash$pi\}\}$\backslash$zeta$\backslash$left($\backslash$frac\{3\}\{2\}$\backslash$right)t+O(1)\}, $\backslash$] where \$$\backslash$zeta\$ is the Riemann zeta-function. Our results have implications for interacting particle systems. The edge scaling limit of the law of real eigenvalues for the real Ginibre ensemble is a rescaling of a fixed time distribution of annihilating Brownian motions (ABM's) with the step initial condition. Therefore, the tail behaviour of the distribution of \$X\_s\^\{(max)\}\$ - the position of the rightmost annihilating particle at fixed time \$s$>$0\$ - can be read off from the corresponding answers for \$$\backslash$lambda\_\{max\}\$ using \$X\_s\^\{(max)\}$\backslash$stackrel\{D\}\{=\} $\backslash$sqrt\{4s\}$\backslash$lambda\_\{max\}\$.},
  annote        = {Comment: 20 pages, expanded introduction, added references},
  archiveprefix = {arXiv},
  journal       = {arXiv:1603.05849 [math-ph]},
  keywords      = {Determinantal, Ginibre, Id{\'e}e},
  month         = mar,
  primaryclass  = {math-ph},
  year          = {2016},
}

