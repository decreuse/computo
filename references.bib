  date          = {2018-01-24},
  title         = {Multivariate approximations in {Wasserstein} distance by {Stein}'s method and {Bismut}'s formula},
  eprint        = {http://arxiv.org/abs/1801.07815v2},
  eprintclass   = {math.PR},
